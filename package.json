{
  "name": "repo-watch",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "dev": "nodemon ./bin/www",
    "bundle2": "NODE_ENV=production browserify  app/client.js -t envify -t babelify -t uglifyify -p [minifyify --no-map] > public/js/bundle.js",
    "bundle": "browserify --debug app/client.js -t babelify --outfile public/js/bundle.js",
    "watch": "watchify -t babelify app/client.js --outfile public/js/bundle.js",
    "plato": "es6-plato -r -d report routes"
  },
  "dependencies": {
    "@octokit/rest": "^16.25.0",
    "@reach/router": "^1.2.1",
    "axios": "^0.18.0",
    "body-parser": "~1.18.3",
    "cookie-parser": "~1.4.4",
    "debug": "~4.1.1",
    "ejs": "~2.6.1",
    "es6-plato": "^1.1.5",
    "es6-promise": "^4.2.6",
    "express": "~4.16.4",
    "history": "^4.7.2",
    "ioredis": "^4.9.0",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.11",
    "morgan": "~1.9.1",
    "parse-link-header": "^1.0.1",
    "prop-types": "^15.7.2",
    "react": "^16.8.2",
    "react-dom": "^16.8.2",
    "react-form-elements": "^2.3.1",
    "serve-favicon": "~2.5.0",
    "xtconf": "^4.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-decorators": "^7.4.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-syntax-function-bind": "^7.2.0",
    "@babel/preset-env": "^7.4.2",
    "@babel/preset-react": "^7.0.0",
    "@kev_nz/eslint-config": "^3.2.1",
    "babelify": "^10.0.0",
    "browserify": "^16.2.3",
    "envify": "^4.1.0",
    "eslint": "^5.16.0",
    "minifyify": "^7.3.5",
    "nodemon": "^1.18.11",
    "uglifyify": "^5.0.1",
    "watchify": "^3.11.1"
  }
}
