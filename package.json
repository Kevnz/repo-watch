{
  "name": "repo-watch",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "dev": "nodemon ./bin/www",
    "bundle2": "NODE_ENV=production browserify  app/client.js -t envify -t babelify -t uglifyify -p [minifyify --no-map] > public/js/bundle.js",
    "bundle": "browserify --debug app/client.js -t babelify --outfile public/js/bundle.js",
    "watch": "watchify -t babelify app/client.js --outfile public/js/bundle.js",
    "plato": "es6-plato -r -d report routes"
  },
  "dependencies": {
    "@kev_nz/async-tools": "^1.2.3",
    "@octokit/rest": "^16.35.0",
    "@reach/router": "^1.2.1",
    "axios": "^0.19.0",
    "body-parser": "~1.19.2",
    "cookie-parser": "~1.4.4",
    "debug": "~4.1.1",
    "ejs": "~2.7.4",
    "es6-plato": "^1.2.3",
    "es6-promise": "^4.2.8",
    "express": "~4.17.3",
    "history": "^4.10.1",
    "ioredis": "^4.14.1",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.15",
    "morgan": "~1.9.1",
    "parse-link-header": "^1.0.1",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-form-elements": "^4.13.2",
    "rollbar": "^2.14.4",
    "serve-favicon": "~2.5.0",
    "xtconf": "^4.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.7.4",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-decorators": "^7.7.4",
    "@babel/plugin-syntax-dynamic-import": "^7.7.4",
    "@babel/plugin-syntax-function-bind": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "@babel/preset-react": "^7.7.4",
    "@kev_nz/eslint-config": "^5.1.0",
    "babelify": "^10.0.0",
    "browserify": "^16.5.0",
    "envify": "^4.1.0",
    "eslint": "^6.7.0",
    "minifyify": "^7.3.5",
    "nodemon": "^2.0.1",
    "uglifyify": "^5.0.2",
    "watchify": "^3.11.1"
  }
}
